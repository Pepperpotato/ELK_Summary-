#SAP
rule_sap1 (?m)%{SAP_TIME:LogTime}%{DATA}(?<rule_sap1>LOG Q0I\=\> NiPConnect2: %{HOSTPORT}: connect \(111: Connection refused\))%{DATA}
rule_sap2 (?<rule_sap2>Unavailable)%{SPACE}%{DATESTAMP}%{SPACE}-%{SPACE}%{DATESTAMP}
rule_sap3 (?m)%{DATA}(?<rule_sap3>ERROR \=\> SSL handshake with %{DATA} failed: SSSLERR_CONN_CLOSED \(-10\))%{DATA}
rule_sap4 (?m)%{SAP_TIME:LogTime}%{DATA}(?<rule_sap4>update not active)%{DATA}
rule_sap5 (?m)%{SAP_TIME:LogTime}%{DATA}(?<rule_sap5>(ERROR in DB6Connect\[/bas/749_REL/src/dbs/db6/dbdb6\.c, 1930\])|(ERROR \=\> DbSlConnect to %{DATA} as %{DATA} failed))
rule_sap6 (?m)%{SAP_TIME:LogTime}%{DATA}(?<rule_sap6>ERROR \=\> AppServerTimeSync, zdate_gettime)%{DATA}
rule_sap7 (?m)%{SAP_TIME:LogTime}%{DATA}(?<rule_sap7>The temporary license key for %{DATA} duration 0 was installed successfully)%{DATA}
rule_sap8 (?m)%{SAP_TIME:LogTime}%{GREEDYDATA:Data}

